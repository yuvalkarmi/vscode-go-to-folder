{
  "name": "go-to-folder",
  "displayName": "Go to Folder - Sidebar",
  "description": "Quickly jump to any folder in the sidebar using fuzzy search. Type partial folder names and instantly jump to them in the sidebar.",
  "keywords": [
    "quick",
    "open folder"
  ],
  "version": "1.1.0",
  "publisher": "yuvalkarmi",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuvalkarmi/vscode-go-to-folder.git"
  },
  "bugs": {
    "url": "https://github.com/yuvalkarmi/vscode-go-to-folder/issues"
  },
  "homepage": "https://github.com/yuvalkarmi/vscode-go-to-folder#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "menus": {
      "commandPalette": [
        {
          "command": "go-to-folder.openFolder"
        }
      ]
    },
    "commands": [
      {
        "command": "go-to-folder.openFolder",
        "title": "Go to Folder..."
      }
    ],
    "keybindings": [
      {
        "command": "go-to-folder.openFolder",
        "key": "ctrl+shift+o",
        "mac": "cmd+shift+o",
        "when": "editorTextFocus || !inputFocus"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js",
    "eslint-fix": "eslint --fix .",
    "prettier-fix": "prettier --write .",
    "_vscode:pack": "vsce package",
    "_vscode:install": "ls go-to-folder-*.vsix | xargs code --install-extension",
    "_vscode:uninstall": "code --uninstall-extension yuvalkarmi.go-to-folder"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@types/glob": "^7.1.3",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^8.2.2",
    "@types/vscode": "^1.59.0",
    "@types/webpack-env": "^1.18.0",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/test-web": "^0.0.56",
    "@vscode/vsce": "^3.6.2",
    "assert": "^2.0.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "prettier": "3.3.3",
    "process": "^0.11.10",
    "ts-loader": "^9.2.6",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "minimatch": "^3.1.2"
  },
  "icon": "images/icon.png"
}
